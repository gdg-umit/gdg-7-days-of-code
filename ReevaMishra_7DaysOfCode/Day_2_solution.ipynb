   "source": [
    "def print_pyramid(N):\n",
    "    for i in range(N):\n",
    "        \n",
    "        spaces = N - i - 1\n",
    "        stars = 2 * i + 1\n",
    "        \n",
    "        print(' ' * spaces + '*' * stars)\n",
    "\n",
    "\n",
    "N = int(input(\"Enter the value of N: \"))\n",
    "print_pyramid(N)\n"
   ]
  }
